{
  "name": "roman-classic-server",
  "version": "0.0.6",
  "description": "Server for roman-classic.com",
  "scripts": {
    "start": "pnpm build && NODE_ENV=production node dist/esm/server.js",
    "staging": "pnpm build && NODE_ENV=staging node dist/esm/server.js",
    "dev": "NODE_ENV=development nodemon ./server.ts --ext js,json,ts",
    "test": "NODE_ENV=test node -r ts-node/register -r tsconfig-paths/register dist/server.js",
    "build:types": "tsc --emitDeclarationOnly --declarationDir dist/esm && tsc-alias",
    "build:dist": "tsc && tsc-alias",
    "build": "NODE_ENV=production pnpm build:dist && NODE_ENV=production pnpm build:types"
  },
  "author": "Ahmed Essam",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.414.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.17.3",
    "helmet": "^7.2.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.10.1",
    "mongoose": "^7.8.6",
    "mongoose-slug-updater": "^3.3.0",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^15.0.0",
    "socket.io": "^4.8.1",
    "winston": "^3.10.0",
    "winston-loki": "^6.0.7"
  },
  "devDependencies": {
    "@swc/core": "^1.3.82",
    "@swc/helpers": "^0.5.1",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.7",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.17.19",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "regenerator-runtime": "^0.14.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "bcrypt",
      "protobufjs"
    ]
  }
}
